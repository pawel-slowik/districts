parameters:
    level: 9
    paths:
        - src
        - tests
        - dependencies
        - bin/console
        - bin/doctrine
    inferPrivatePropertyTypeFromConstructor: true
    stubFiles:
        - stubs/Container.php

includes:
    - /opt/phpstan/vendor/phpstan/phpstan-doctrine/extension.neon
    - /opt/phpstan/vendor/phpstan/phpstan-phpunit/extension.neon
