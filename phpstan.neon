parameters:
    level: 9
    paths:
        - src
        - tests
        - dependencies
        - bin/console
        - bin/doctrine
    inferPrivatePropertyTypeFromConstructor: true
    ignoreErrors:
        - identifier: missingType.iterableValue

includes:
    - /phpstan/vendor/phpstan/phpstan-doctrine/extension.neon
    - /phpstan/vendor/phpstan/phpstan-phpunit/extension.neon
